    "scripts": {
        "start": "node server",
        "deploy-backend": "git subtree push --prefix backend carmeets-backend master",
        "build-frontend": "cd frontend && ng build --prod",
        "commit-build-changes": "git add . && git commit -m 'PushToHeroku'",
        "deploy-frontend": "npm run build-frontend && npm run copy && npm run commit-build-changes && git subtree push --prefix serve-frontend carmeets-frontend master",
        "copy": "cpx 'frontend/dist/**' serve-frontend/www"
    },
